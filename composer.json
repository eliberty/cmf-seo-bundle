{
    "name": "eliberty/cmf-seo-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Search Engine Optimization Bundle",
    "keywords": [
        "Symfony CMF",
        "SEO"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/SeoBundle/contributors"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://e-liberty.repo.repman.rpstack.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "sonata-project/seo-bundle": "2.10.0",
        "doctrine/collections": "^1.0"
    },
    "require-dev": {
        "doctrine/phpcr-odm": "^1.4.2 || ^2.0",
        "doctrine/orm": "^2.4",
        "eliberty/cmf-core-bundle": "^2.500",
        "eliberty/cmf-routing-bundle": "^2.500",
        "matthiasnoback/symfony-dependency-injection-test": "^1.1",
        "matthiasnoback/symfony-config-test": "^2.1",
        "symfony/phpunit-bridge": "^3.4 || ^4.0",
        "burgov/key-value-form-bundle": "^1.0",
        "symfony-cmf/testing": "^2.1.9",
        "symfony/security-bundle": "^3.4 || ^4.0",
        "symfony/translation": "^3.4 || ^4.0",
        "symfony/validator": "^3.4 || ^4.0",
        "symfony/asset": "^3.4 || ^4.0",
        "symfony/templating": "^3.4 || ^4.0",
        "symfony/browser-kit": "^3.4 || ^4.0",
        "symfony/css-selector": "^3.4 || ^4.0",
        "rector/rector": "^0.15.21",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\SeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\SeoBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "conflict": {
        "phpcr/phpcr-utils": "<1.3.0",
        "sebastian/exporter": "<2.0.0",
        "burgov/key-value-form-bundle": "<1.3.1"
    }
}
